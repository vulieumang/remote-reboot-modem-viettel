{
    "manifest_version": 3,
    "name": "1-Click Modem Reboot",
    "version": "1.0",
    "description": "Reboot Viettel Modem with one click",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}